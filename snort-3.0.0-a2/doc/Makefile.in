# FIXIT no fuschia for visited; all should be sans serif extracted
# asciidoc.css doesn't seem to work with -a linkcss asciidoc native html
# generation; buggy passthru of base64 data.
@MAKE_HTML_DOC_TRUE@snort_manual.html: $(all_sources)
@MAKE_HTML_DOC_TRUE@	a2x -f chunked $(html_args) $(snort_src)
@MAKE_HTML_DOC_TRUE@	tar zcf snort_manual.tgz snort_manual.chunked
@MAKE_HTML_DOC_TRUE@	asciidoc -b xhtml11 -a toc2 -a icons -a imagesdir=. $(snort_src)
@MAKE_HTML_DOC_FALSE@snort_manual.html: default_snort_manual.html
@MAKE_HTML_DOC_FALSE@	cp $< $@

# a2x generates an intermediate xml file unconditionally and always places
# it in this dir even if the output is put elsewhere, which breaks parallel
# builds.  hence we just build all sequentially, which is why dependencies
# are:  text depends on pdf depends on html.
@MAKE_PDF_DOC_TRUE@snort_manual.pdf: $(all_sources) snort_manual.html
@MAKE_PDF_DOC_TRUE@	a2x -f pdf $(pdf_args) $(snort_src)
@MAKE_PDF_DOC_FALSE@snort_manual.pdf: default_snort_manual.pdf snort_manual.html
@MAKE_PDF_DOC_FALSE@	cp $< $@

@MAKE_TEXT_DOC_TRUE@snort_manual.text: $(all_sources) snort_manual.pdf
@MAKE_TEXT_DOC_TRUE@	a2x -f text $(snort_src)
@MAKE_TEXT_DOC_FALSE@snort_manual.text: default_snort_manual.text snort_manual.pdf
@MAKE_TEXT_DOC_FALSE@	cp $< $@

# autotools makes doesn't seem to like directory targets, especially ones
# with an unspecified set of files.  to avoid these issues, we generate
# a tarball and then explode that upon install with a hook.  note that
# there is no default for chunked.
@MAKE_HTML_DOC_TRUE@install-data-hook:
@MAKE_HTML_DOC_TRUE@	tar zxf $(builddir)/snort_manual.tgz -C $(DESTDIR)$(docdir)/

@MAKE_HTML_DOC_TRUE@uninstall-hook:
@MAKE_HTML_DOC_TRUE@	rm -rf $(DESTDIR)$(docdir)/snort_manual.chunked

@MAKE_HTML_DOC_TRUE@snort_online.html: $(all_sources)
@MAKE_HTML_DOC_TRUE@	$(srcdir)/online_manual.sh

@MAKE_HTML_DOC_TRUE@dev_guide.html: $(snort)
@MAKE_HTML_DOC_TRUE@	$(srcdir)/dev_guide.sh $(top_srcdir)

clean-local:
	rm -f $(BUILT_SOURCES)
	rm -f $(delete_unbuilt)
	rm -f $(doc_DATA)
	rm -f snort_manual.{fo,xml}
	rm -rf snort_manual.chunked
	rm -f *.css *.tgz
	rm -f {online_manual,dev_guide}.html

# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
